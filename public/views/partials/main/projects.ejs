<%- contentFor('top')%>
<link rel="stylesheet" href="css/skills.css">
<link rel="stylesheet" href="css/projects.css">
<%- contentFor('main') %>
<div class="container__main">
    <div class="container__main__title">
        <h1 class="main__title">skills</h1> 
        <hr class="main__split">
    </div>
    <div class="container__skills">
        <img class="icon__skills" src="https://logos-download.com/wp-content/uploads/2019/01/JavaScript_Logo-700x700.png" alt="js">
        <img class="icon__skills" src="http://mspoweruser.com/wp-content/uploads/msn/2015/03/MonoGame_Logo.svg_.png" alt="monogame">
        <img class="icon__skills" src="./img/skills/csharp.png" alt="csharp">
        <img class="icon__skills" src="./img/skills/css.png" alt="css">
        <img class="icon__skills" src="./img/skills/html.png" alt="html">
        <img class="icon__skills" src="./img/skills/godot.png" alt="godot">
        <img class="icon__skills" src="./img/skills/java.png" alt="java">
        <img class="icon__skills" src="./img/skills/mongo.png" alt="mongo">
        <img class="icon__skills" src="./img/skills/nodejs.png" alt="nodejs">
        <img class="icon__skills" src="./img/skills/python.png" alt="python">
    </div>
</div>
<div class="container__main">
    <div class="container__main__title">
        <h1 class="main__title">proyectos</h1> 
        <hr class="main__split">
    </div>
    <div class="container__projects">
        <%projects.forEach(element=>{ %>
        <%-include('../component/project.ejs', {project: element}) %>
        <%})%>
    </div>
    
</div>
